//- Commonly used meta list
//- ---------------------------------------------------------

//- Declare the character encoding
meta(charset='utf-8')

//- Using the latest version of IE and Chrome
meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")

//- Page description
meta(name='description', content="Social share demo")

//- Page Keyword
meta(name='keywords', content="social, share, demo")

//- author
meta(name="author", content="cycjimmy, cycjimmy@gmail.com")

//- for search engine
meta(name="robots", content="index")


//- speed mode for the 360 browser
meta(name="renderer", content="webkit")





